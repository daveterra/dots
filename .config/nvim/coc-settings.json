  "diagnostic.checkCurrentLine": true,
  "diagnostic.messageDelay": 0,
  "diagnostic.virtualText": true,
  "diagnostic.messageTarget": "echo",
  "diagnostic.virtualTextPrefix": ">>",
  "diagnostic.refreshOnInsertMode": false,
  "diagnostic.errorSign": "✘",
  "diagnostic.warningSign": "",
  "diagnostic.infoSign": "",
  "diagnostic.hintSign": "",
  "diagnostic.enableMessage":" always",

  "coc.preferences.willSaveHandlerTimeout": 5000,
  "coc.preferences.formatOnType": true,
  "coc.preferences.formatOnSaveFiletypes": [
    "python",
    "javascript",
    "javascriptreact",
    "typescriptreact",
    "css"
  ],

  "python.venvPath": "/Users/dave/.virtualenvs",
  "pyright.organizeimports.provider": "isort",
  "pyright.completion.snippetSupport": true,
  "python.pythonPath": "python",
  "python.formatting.provider": "black",
  "python.formatting.blackPath": "black",
  "python.formatting.blackArgs": ["--line-length=100"],

  "javascript.format.enabled": true,
  "javascript.showUnused": true,

  "yaml.enable": true,
  "yaml.format.enable": true,
  "yaml.format.printWidth": 80,
  "yaml.format.proseWrap": "always",
  "yaml.format.singleQuote": true,
  "yaml.format.bracketSpacing": true,
  "yaml.schemaStore.enable": true,
  "yaml.schemas": {
    "https://raw.githubusercontent.com/compose-spec/compose-spec/master/schema/compose-spec.json": "**/docker-compose2*.yml"
  },

  "hover.autoHide": false,
  "hover.target": "float",

  "clangd.arguments": ["--enable-config"],

  "suggest.enableFloat": true,
  "suggest.noselect": false,
  "suggest.acceptSuggestionOnCommitCharacter": true,
  "suggest.enablePreview": true,
  "suggest.enablePreselect": true,
  "coc.preferences.currentFunctionSymbolAutoUpdate": true,
